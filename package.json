{
  "name": "crypto-flash",
  "version": "0.1.0",
  "description": "send me some eth, how bout dat?",
  "dependencies": {
    "@ensdomains/ens": "0.3.3",
    "@netgum/utils": "0.0.5",
    "axios": "^0.18.0",
    "bip39": "^2.5.0",
    "core-js": "^2.6.5",
    "eth-lib": "^0.2.8",
    "ethereumjs-wallet": "^0.6.3",
    "express": "4.16.4",
    "openzeppelin-solidity": "2.1.2",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-router-dom": "4.3.1",
    "truffle-contract": "^4.0.5",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "cross-env": "5.2.0",
    "ganache-cli": "6.3.0-beta.0",
    "parcel-bundler": "1.11.0",
    "styled-components": "4.0.0",
    "truffle": "5.0.4",
    "truffle-hdwallet-provider": "1.0.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel app/index.html ",
    "build": "cross-env NODE_ENV=development parcel build app/index.html --no-minify --public-url /",
    "service": "node service/main.js",
    "cleanup:build": "rm -rf build",
    "cleanup:dist": "rm -rf dist",
    "compile": "npm run cleanup:build && truffle compile --all",
    "migrate": "truffle migrate",
    "migrate-rinkeby": "truffle migrate --network=rinkeby",
    "deploy": "npm run migrate && truffle exec scripts/deploy_dependencies.js",
    "deploy-rinkeby": "npm run migrate-rinkeby && truffle exec scripts/deploy_dependencies.js --network=rinkeby",
    "deploy-ens": "truffle exec scripts/deploy_ens.js --network=rinkeby",
    "ganache": "scripts/run_ganache.sh"
  }
}
